{
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/sitemap.xml",
            "destination": "/api/sitemap"
        },
        {
            "source": "/((?!api/|.*\\.(?:png|jpg|jpeg|gif|svg|ico|webp)$|favicon\\.ico).*)",
            "destination": "/index.html"
        }
    ]
}